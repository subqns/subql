{
  "name": "@subquery/node",
  "version": "0.13.4",
  "description": "",
  "author": "Ian He",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "codegen": "graphql-codegen --config codegen.yml",
    "build": "rm -rf dist && tsc -b",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "dev:node": "env APP=node ts-node-dev --respawn --transpile-only src/main.ts",
    "dev:query": "env APP=query ts-node-dev --respawn --transpile-only  src/main.ts",
    "start": "ts-node src/main.ts",
    "start:node": "env APP=node ts-node src/main.ts",
    "start:query": "env APP=query ts-node src/main.ts",
    "start:nest": "nest start",
    "start:dev": "nodemon",
    "start:prod": "node dist/main",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "homepage": "https://github.com/subquery/subql",
  "repository": "github:subquery/subql",
  "bin": {
    "subquery-node": "./bin/run"
  },
  "dependencies": {
    "@graphile-contrib/pg-many-to-many": "^1.0.1",
    "@nestjs/common": "8.0.6",
    "@nestjs/core": "8.0.6",
    "@nestjs/event-emitter": "1.0.0",
    "@nestjs/platform-express": "8.0.6",
    "@nestjs/schedule": "1.0.1",
    "@nestjs/swagger": "5.0.9",
    "@nestjs/typeorm": "8.0.2",
    "@nftmart/subql": "workspace:*",
    "@polkadot/api": "latest",
    "@subquery/common": "workspace:*",
    "@subquery/types": "workspace:*",
    "@willsoto/nestjs-prometheus": "^3.0.0",
    "app-module-path": "^2.2.0",
    "bn.js": "^5.2.0",
    "dayjs": "^1.10.4",
    "express-graphql": "^0.12.0",
    "graphile-utils": "^4.12.1",
    "graphql-middleware": "^6.0.10",
    "graphql-shield": "^7.5.0",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "parse-json": "^5.2.0",
    "pg": "^8.7.1",
    "pino": "^6.11.1",
    "postgraphile": "^4.12.3",
